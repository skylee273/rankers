<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="viewModel"
            type="com.project.rankers.ui.leagueResult.LeagueResultItemViewModel"/>
    </data>

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="16dp"
    android:layout_marginRight="16dp"
    android:layout_marginBottom="16dp">

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@drawable/costom_edit_text_bg"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4">

        <TextView
            android:id="@+id/text_player8"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@{viewModel.player8}"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical" />

        <TextView
            android:id="@+id/text_score10"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical" />

        <TextView
            android:id="@+id/text_score11"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical" />

        <TextView
            android:id="@+id/text_score12"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/costom_x_box"
            android:gravity="center_horizontal|center_vertical" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@drawable/costom_edit_text_bg"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

        <TextView
            android:id="@+id/text_player7"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@{viewModel.player7}"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical" />

        <TextView
            android:id="@+id/text_score7"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical" />

        <TextView
            android:id="@+id/text_score8"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/costom_x_box"
            android:gravity="center_horizontal|center_vertical" />

        <TextView
            android:id="@+id/text_score9"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:onClick="@{() -> viewModel.onScore9Click()}"
            android:text="@{viewModel.score9}"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@drawable/costom_edit_text_bg"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2">

        <TextView
            android:id="@+id/text_player6"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@{viewModel.player6}"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical" />

        <TextView
            android:id="@+id/text_score4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/costom_x_box"
            android:gravity="center_horizontal|center_vertical" />

        <TextView
            android:id="@+id/text_score5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:onClick="@{() -> viewModel.onScore5Click()}"
            android:text="@{viewModel.score5}"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical" />

        <TextView
            android:id="@+id/text_score6"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:onClick="@{() -> viewModel.onScore6Click()}"
            android:text="@{viewModel.score6}"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@drawable/costom_edit_text_bg"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout">

        <TextView
            android:id="@+id/text_player5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@{viewModel.player5}"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/costom_x_box"
            android:gravity="center_horizontal|center_vertical" />

        <TextView
            android:id="@+id/text_score1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical"
            android:onClick="@{() -> viewModel.onScore1Click()}"
            android:text="@{viewModel.score1}" />

        <TextView
            android:id="@+id/text_score2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical"
            android:onClick="@{() -> viewModel.onScore2Click()}"
            android:text="@{viewModel.score2}" />

        <TextView
            android:id="@+id/text_score3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:onClick="@{() -> viewModel.onScore3Click()}"
            android:text="@{viewModel.score3}"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@drawable/costom_edit_text_bg"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/text_group_number"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical"
            android:text="@{viewModel.number}"
            android:textColor="@color/color_contest_title" />

        <TextView
            android:id="@+id/text_player1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@{viewModel.player1}"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical" />

        <TextView
            android:id="@+id/text_player2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@{viewModel.player2}"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical" />

        <TextView
            android:id="@+id/text_player3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@{viewModel.player3}"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical" />

        <TextView
            android:id="@+id/text_player4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@{viewModel.player4}"
            android:background="@drawable/costom_edit_text_bg"
            android:gravity="center_horizontal|center_vertical" />
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>

</layout>